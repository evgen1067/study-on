<form method="post" action="{{ path('app_course_delete', {'id': course.id}) }}"
      onsubmit="return confirm('Вы действительно хотите удалить данный курс?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ course.id) }}">
    <button class="btn btn-danger w-100 mb-3">Удалить</button>
</form>